<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.sample.birthday">
	<author>Ryan Park</author>
	<namespace>Sample_Birthday</namespace>
	<entity name="Birthday" local-service="true" remote-service="false">
		<column name="birthdayId" type="long" primary="true" />
		<column name="name" type="String" />
		<column name="date" type="Date" />
	</entity>
</service-builder>